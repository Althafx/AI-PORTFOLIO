{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install --legacy-peer-deps --no-package-lock",
    "outputDirectory": "dist",
    "framework": "vite",
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}