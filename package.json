{
    "name": "ai-portfolio",
    "version": "1.0.0",
    "description": "AI-powered portfolio with MERN stack",
    "private": true,
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "client": "cd client && npm run dev",
        "server": "cd server && npm run dev",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "install-all": "npm install && cd client && npm install && cd ../server && npm install"
    },
    "keywords": [
        "portfolio",
        "mern",
        "ai",
        "gemini"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}